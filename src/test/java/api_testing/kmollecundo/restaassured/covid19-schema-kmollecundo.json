{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "country": {
          "type": "string"
        },
        "provinces": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "province": {
                  "type": "string"
                },
                "confirmed": {
                  "type": "integer"
                },
                "recovered": {
                  "type": "integer"
                },
                "deaths": {
                  "type": "integer"
                },
                "active": {
                  "type": "integer"
                }
              },
              "required": [
                "province",
                "confirmed",
                "recovered",
                "deaths",
                "active"
              ]
            }
          ]
        }
      }
    }
  ]
}